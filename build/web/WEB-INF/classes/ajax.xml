<struts>
    <package name="ajax" extends="default" namespace="/ajax">

        <default-interceptor-ref name="crudStack"/>
       
        <action name="getProtocolDetails" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="getProtocolDetails"/>   
        <action name="isExistedPartnerName" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="isExistedPartnerName"/>
        <action name="isExistedUserEmail" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="isExistedUserEmail"/>
        <action name="isExistedAS2PartnerProfileName" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="isExistedAS2PartnerProfileName"/>
    <action name="addPartner" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="addPartner"/>
    </package>
</struts>
