<struts>
    <package name="ajax" extends="default" namespace="/ajax">

         <default-interceptor-ref name="crudStack"/>
          <action name="getPoDetails" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="getPoDetails"/>
          <action name="getASNDetails" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="getAsnDetails"/>
          <action name="getInvDetails" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="getInvDetails"/>
          <action name="getPaymentDetails" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="getPaymentDetails"/>
        
         <action name="getDocDetails" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="getDocDetails"/>
         <action name="getDocCopy" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="getDocCopy"/>
          <action name="getLoadCopy" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="getLoadCopy"/>
         
         
         <!-- Life cycle Actions -->
         <action name="LifecycleDetails" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="getLifecycleDetails"/>
         
         <!-- TP Actions  -->
         
         <action name="getTpDetails" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="getTpDetails"/>
         <action name="updateTpDetails" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="updateTpDetails"/>
         <action name="getTpDetailsByName" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="getTpDetailsByName"/>
         <action name="getTpDetailInformation" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="getTpDetailInformation"/>
         <action name="getLogisticsDocDetails" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="getLogisticsDocDetails"/>
         <action name="getLoadTenderingDetails" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="getLoadTenderingDetails"/>
         <action name="getLtResponseDetails" class="com.mss.ediscv.ajax.AjaxHandlerAction" method="getLtResponseDetails"/>
         
    </package>
</struts>
